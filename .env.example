# API endpoint URL
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Cache Time-to-Live for API responses in milliseconds
# 15 minutes = 15 * 60 * 1000 = 900000
NEXT_PUBLIC_API_CACHE_TTL=900000

# Cache cleanup interval in milliseconds
# 5 minutes = 5 * 60 * 1000 = 300000
NEXT_PUBLIC_CACHE_CLEANUP_INTERVAL=300000